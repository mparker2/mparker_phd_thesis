journal = {Biochemical Journal},
keywords = {5-untranslated region (5-UTR),ATAXIA TELANGIECTASIA-MUTATED AND RAD3-RELATED (AT,Arabidopsis thaliana,RNA structure,guanine quadruplex structure (GQS),translation},
month = {apr},
number = {1},
pages = {91--102},
pmid = {25793418},
title = {{A stable RNA G-quadruplex within the 5′-UTR of Arabidopsis thaliana ATR mRNA inhibits translation}},
