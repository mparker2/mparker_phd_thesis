doi = {10.1073/pnas.48.12.2013},
file = {:home/matthew/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/GELLERT, LIPSETT, DAVIES - 1962 - Helix formation by guanylic acid.pdf:pdf},
isbn = {0027-8424 (Print) 0027-8424 (Linking)},
issn = {0027-8424},
journal = {Proceedings of the National Academy of Sciences},
keywords = {GUANINE NUCLEOTIDES},
month = {dec},
number = {12},
pages = {2013--2018},
pmid = {13947099},
publisher = {National Academy of Sciences},
title = {{HELIX FORMATION BY GUANYLIC ACID}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/13947099 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC221115 http://www.pnas.org/cgi/doi/10.1073/pnas.48.12.2013},
