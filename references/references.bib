keywords = {GC biased gene conversion,GC content,angiosperm genomes,intron/exon structure,methylation,recombination},
month = {jul},
number = {7},
pages = {263--70},
pmid = {24916172},
publisher = {Elsevier},
title = {{GC content evolution in coding regions of angiosperm genomes: a unifying hypothesis.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/24916172},
volume = {30},
year = {2014}
}
