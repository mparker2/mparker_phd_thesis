publisher = {Frontiers},
title = {{G Quadruplex in Plants: A Ubiquitous Regulatory Element and Its Biological Relevance}},
url = {http://journal.frontiersin.org/article/10.3389/fpls.2017.01163/full},
volume = {8},
year = {2017}
}
