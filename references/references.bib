pmid = {10362140},
title = {{Berberine-induced apoptosis of human leukemia HL-60 cells is associated with down-regulation of nucleophosmin/B23 and telomerase activity}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/10362140},
volume = {81},
year = {1999}
}
