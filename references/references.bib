@article{Reaban1994,
abstract = {W e previously showed that the in vitro transcription of a negatively supercoiled plasmid containing the mu-rine IgA switch region caused the formation of fewer supercoiled conformers of the plasmid due to the presence of a stable RNA-DNA hybrid. Here, we demonstrate that the RNA-DNA hybrid is approximately 140 nucleotides , and it forms regardless of the initial topological state of the transcription template. Transcription of the switch region in a relaxed closed circular plasmid generates positively supercoiled plasmid conformers that revert to their original relaxed state when treated with RNase H. Conformers that have incorporated a stable RNA transcript are also observed when nicked circular and linear plasmids containing the IgA switch sequences are transcribed with "P-labeled nucleotide triphosphates. Once formed, the RNA-DNA hybrid is stable to both thermal and superhelical stress, tolerating temperatures in excess of 95 "C and restraining approximately 12 positive supercoils in the plasmid. Immunoglobulin class switching is mediated by recombina-tion between two DNA sequences called switch regions located 3' of the variable region and 5' of the new constant region that will be expressed. These sequences are associated with conserved transcription promoters. Transcriptional activity has been correlated with increased recombination rates in such diverse systems as the immunoglobulin variable region assembly process (1) and the Saccharomyces cerevisiae GAL 10 locus (2). Transcription may also play an important role in immuno-globulin class switch recombination, because the specific switch regions where the DNA recombination will occur become tran-scriptionally active immediately prior t o class switching (3). Transcription might enhance recombination via several mechanisms. First, transcription may serve to "open up" the transcribed DNA to make it more accessible to the recombination machinery. Second, it may transiently generate positively and negatively supercoiled domains in the transcribed DNA (4-6) affecting both structural transitions and interactions between DNA and other molecules that contribute t o recombination. Third, the transcription just prior to switch recombination also generates "sterile transcripts," and our data suggest that these transcripts may participate directly in the formation of a re-combination intermediate. The switch region located 5' of murine IgA constant region contains an (AGGAG),, repeat that displays supercoil-dependent sensitivity to single-strand specific reagents. The reactivity pattern that we observed in a shorter repeat, (AGGAG),GA, was consistent with the formation of a pyrpur'pyr intramo-lecular triplex (7). When we transcribed the switch region-containing plasmid, pBS-(rS2.3, with T7 RNA polymerase in the same direction as transcription occurs in vivo prior to switch recombination, a stable RNA.DNA hybrid was generated. Formation of this hybrid structure relaxed negatively supercoiled plasmids, generating a ladder of less supercoiled plasmid conformers that were not generated when the opposite strand of pBS-aS2.3 was transcribed with T3 RNA polymerase (8, 9). The stretch of polypurine-polypyrimidine sequences in which this RNA.DNA hybrid structure forms is known to have the ability to form a variety of interesting alternative structures. Under appropriate in uitro conditions, pyr.pur.pyr (10, 11) or pyr.pur.pur (12, 13) co-linear triplexes, pyr.pur.pyr (14) or pyr.pur.pur (15) intramolecular triplexes, or fused double tri-plex structures (16) have been shown to form. Polypurine-poly-pyrimidine stretches occur more frequently in eukaryotic DNA than can be explained by random distribution, and they are frequently found in regions associated with gene regulation or DNA recombination (17). Examples include the human c-myc (12, 18) and murine metallothionein-I (19) gene regulatory regions , a factor binding region of the chicken p-globin locus (201, the site of an unequal sister chromatid exchange in a myeloma cell line (21), and the murine IgA switch region (71, all of which have been shown capable of adopting alternative structures in uitro. These observations, together with data presented in this article , begin to reveal some insight into the mechanism of im-munoglobulin switch recombination. They suggest the possibility that the transcription of switch regions, which precedes DNA rearrangement, leads to the synthesis of RNA transcripts that become an integral part of the recombination intermediate structure. MATERIALS AND METHODS Chemicals and Enzymes-Chloracetaldehyde (CAA),' which reacts with unpaired adenosine and cytosine residues, was purchased from Fluka and was distilled twice. Fractions were collected between 85 and 88 "C, diluted to 3 M, and stored at-20 "C. Mung bean nuclease and nuclease S1, which hydrolyze both single-stranded RNAand DNA, were purchased from New England Biolabs and U. S. Biochemical Corp., respectively. RNase A, which cleaves single-stranded RNA at pyrimi-dine residues, was purchased from Calbiochem, resuspended in water, and boiled to remove any contaminating DNase activity. RNase H, which degrades the RNA in an RNA.DNA hybrid, was purchased from Boehringer-Mannheim. AmpliTaq DNA polymerase was purchased from Perkin-Elmer and T3 and T7 RNA polymerases from International The abbreviation used is: CAA, chloracetaldehyde. 21850},
author = {Reaban, M E and Lebowitz, J and Griffin, J A},
file = {::},
journal = {Journal of Biological Chemistry},
number = {34},
pages = {21850--21857},
title = {{Transcription induces the formation of a stable RNA-center-dot-DNA hybrid in the immunoglobulin-alpha switch region}},
url = {http://www.jbc.org/content/269/34/21850.full.pdf},
volume = {269},
year = {1994}
}
@article{Pearson2009,
abstract = {BACKGROUND:Most analyses of microarray data are based on point estimates of expression levels and ignore the uncertainty of such estimates. By determining uncertainties from Affymetrix GeneChip data and propagating these uncertainties to downstream analyses it has been shown that we can improve results of differential expression detection, principal component analysis and clustering. Previously, implementations of these uncertainty propagation methods have only been available as separate packages, written in different languages. Previous implementations have also suffered from being very costly to compute, and in the case of differential expression detection, have been limited in the experimental designs to which they can be applied.RESULTS:puma is a Bioconductor package incorporating a suite of analysis methods for use on Affymetrix GeneChip data. puma extends the differential expression detection methods of previous work from the 2-class case to the multi-factorial case. puma can be used to automatically create design and contrast matrices for typical experimental designs, which can be used both within the package itself but also in other Bioconductor packages. The implementation of differential expression detection methods has been parallelised leading to significant decreases in processing time on a range of computer architectures. puma incorporates the first R implementation of an uncertainty propagation version of principal component analysis, and an implementation of a clustering method based on uncertainty propagation. All of these techniques are brought together in a single, easy-to-use package with clear, task-based documentation.CONCLUSION:For the first time, the puma package makes a suite of uncertainty propagation methods available to a general audience. These methods can be used to improve results from more traditional analyses of microarray data. puma also offers improvements in terms of scope and speed of execution over previously available methods. puma is recommended for anyone working with the Affymetrix GeneChip platform for gene expression analysis and can also be applied more generally.},
author = {Pearson, Richard D and Liu, Xuejun and Sanguinetti, Guido and Milo, Marta and Lawrence, Neil D and Rattray, Magnus},
doi = {10.1186/1471-2105-10-211},
file = {::},
isbn = {1471210510211},
issn = {14712105},
journal = {BMC Bioinformatics},
keywords = {Algorithms,Bioinformatics,Combinatorial Libraries,Computational Biology/Bioinformatics,Computer Appl. in Life Sciences,Microarrays},
month = {jul},
number = {1},
pages = {211},
pmid = {19589155},
publisher = {BioMed Central},
title = {{puma: A Bioconductor package for propagating uncertainty in microarray analysis}},
url = {http://www.biomedcentral.com/1471-2105/10/211},
volume = {10},
year = {2009}
}
@article{Sims2014,
abstract = {Computational genomics seeks to draw biological inferences from genomic datasets, often by integrating and contextualizing next-generation sequencing data. CGAT provides an extensive suite of tools designed to assist in the analysis of genome scale data from a range of standard file formats. The toolkit enables filtering, comparison, conversion, summarization and annotation of genomic intervals, gene sets and sequences. The tools can both be run from the Unix command line and installed into visual workflow builders, such as Galaxy.},
