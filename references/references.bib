journal = {Proceedings of the National Academy of Sciences},
keywords = {Poaceae,genome size evolution,geographical stratification,phylogenetic regression,plant genome},
month = {sep},
number = {39},
pages = {E4096--E4102},
pmid = {25225383},
publisher = {National Academy of Sciences},
title = {{Ecological and evolutionary significance of genomic GC content diversity in monocots}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/25225383 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC4191780 http://www.pnas.org/lookup/doi/10.1073/pnas.1321152111},
