issn = {03051048},
journal = {Nucleic Acids Research},
keywords = {chickens,chromatin,down-regulation,genes,genetic,genome,oncogenes,open reading frames,rna,saccharomyces cerevisiae,telomere,transcription},
month = {jan},
number = {1},
pages = {144--156},
pmid = {17999996},
publisher = {Oxford University Press},
title = {{Genomic distribution and functional analyses of potential G-quadruplex-forming sequences in Saccharomyces cerevisiae}},
