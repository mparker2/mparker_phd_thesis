file = {:home/matthew/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Rouleau et al. - 2017 - 3' UTR G-quadruplexes regulate miRNA binding.pdf:pdf},
issn = {1469-9001},
