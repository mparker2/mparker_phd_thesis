number = {1},
pages = {192},
publisher = {BioMed Central},
title = {{uShuffle: A useful tool for shuffling biological sequences while preserving the k-let counts}},
url = {http://bmcbioinformatics.biomedcentral.com/articles/10.1186/1471-2105-9-192},
volume = {9},
year = {2008}
}
