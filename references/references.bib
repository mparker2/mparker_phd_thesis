pmid = {27708011},
publisher = {American Association for the Advancement of Science},
title = {{RNA G-quadruplexes are globally unfolded in eukaryotic cells and depleted in bacteria}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/27708011 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC5367264},
