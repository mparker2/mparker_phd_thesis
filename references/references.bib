volume = {39},
year = {2004}
}
@article{Ginno2012,
abstract = {CpG islands (CGIs) function as promoters for approximately 60{\%} of human genes. Most of these elements remain protected from CpG methylation, a prevalent epigenetic modification associated with transcriptional silencing. Here, we report that methylation-resistant CGI promoters are characterized by significant strand asymmetry in the distribution of guanines and cytosines (GC skew) immediately downstream from their transcription start sites. Using innovative genomics methodologies, we show that transcription through regions of GC skew leads to the formation of long R loop structures. Furthermore, we show that GC skew and R loop formation potential is correlated with and predictive of the unmethylated state of CGIs. Finally, we provide evidence that R loop formation protects from DNMT3B1, the primary de novo DNA methyltransferase in early development. Altogether, these results suggest that protection from DNA methylation is a built-in characteristic of the DNA sequence of CGI promoters that is revealed by the cotranscriptional formation of R loop structures. {\textcopyright} 2012 Elsevier Inc..},
archivePrefix = {arXiv},
arxivId = {1311.1716},
