pages = {1485--1498},
pmid = {22021381},
publisher = {Oxford University Press},
title = {{FANCJ coordinates two pathways that maintain epigenetic stability at G-quadruplex DNA}},
url = {https://academic.oup.com/nar/article-lookup/doi/10.1093/nar/gkr868},
volume = {40},
year = {2012}
}
