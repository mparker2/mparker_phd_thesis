issn = {15495469},
journal = {Genome Research},
month = {jul},
number = {7},
pages = {995--1007},
pmid = {25934563},
publisher = {Cold Spring Harbor Laboratory Press},
title = {{Unmasking alternative splicing inside protein-coding exons defines exitrons and their role in proteome plasticity}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/25934563 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC4484396},
volume = {25},
year = {2015}
}
