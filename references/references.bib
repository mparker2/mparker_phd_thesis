issn = {00280836},
journal = {Nature},
keywords = {RNA,Sequencing},
month = {jan},
number = {7485},
pages = {701--705},
pmid = {24336214},
publisher = {Nature Publishing Group},
title = {{Genome-wide probing of RNA structure reveals active unfolding of mRNA structures in vivo}},
url = {http://www.nature.com/articles/nature12894},
volume = {505},
year = {2014}
}
