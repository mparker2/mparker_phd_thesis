issn = {13891723},
journal = {Journal of Bioscience and Bioengineering},
keywords = {Arabidopsis thaliana,Bioinformatics,G-quadruplex,Oryza sativa,Populus trichocarpa,Vitis vinifera},
month = {nov},
number = {5},
pages = {570--575},
pmid = {22721688},
publisher = {Elsevier},
title = {{Discovery of novel rules for G-quadruplex-forming sequences in plants by using bioinformatics methods}},
