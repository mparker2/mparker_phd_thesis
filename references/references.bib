pages = {485--513},
pmid = {20395450},
publisher = {American Society of Plant Biologists},
title = {{A Bioinformatics Approach to the Identification, Classification, and Analysis of Hydroxyproline-Rich Glycoproteins}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/20395450 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC2879790 http://www.plantphysiol.org/cgi/doi/10.1104/pp.110.156554},
