pages = {12316--12321},
pmid = {27729530},
publisher = {National Academy of Sciences},
title = {{Nascent RNA sequencing reveals distinct features in plant transcription.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/27729530 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC5087027},
volume = {113},
year = {2016}
}
