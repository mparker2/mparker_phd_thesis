journal = {Proceedings of the National Academy of Sciences of the United States of America},
keywords = {Genome,Homeodomain Proteins,Homeodomain Proteins: genetics,Plant,Plant Physiological Phenomena,Plant Proteins,Plant Proteins: genetics,Reverse Transcriptase Polymerase Chain Reaction},
number = {6},
pages = {1775--80},
pmid = {14736918},
title = {{Genomic identification of direct target genes of LEAFY.}},
url = {www.ncbi.nlm.nih.govgeo http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=341852{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {101},
year = {2004}
}
@article{Compe2016,
abstract = {Transcription factor IIH (TFIIH) is a multiprotein complex involved in both transcription and DNA repair, revealing a striking functional link be-tween these two processes. Some of its subunits also belong to complexes involved in other cellular processes, such as chromosome segregation and cell cycle regulation, emphasizing the multitasking capabilities of this factor. This review aims to depict the structure of TFIIH and to dissect the roles of its subunits in different cellular mechanisms. Our understanding of the biochemistry of TFIIH has greatly benefited from studies focused on dis-eases related to TFIIH mutations. We address the etiology of these disorders and underline the fact that TFIIH can be considered a promising target for therapeutic strategies.},
