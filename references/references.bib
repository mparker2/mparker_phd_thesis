pmid = {17158149},
publisher = {Oxford University Press},
title = {{Different levels of alternative splicing among eukaryotes}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/17158149 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC1802581},
volume = {35},
year = {2007}
}
