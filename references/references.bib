isbn = {1545-4509 (Electronic)$\backslash$n0066-4154 (Linking)},
issn = {0066-4154},
journal = {Annual Review of Biochemistry},
keywords = {eIFs,initiation,ribosome,scanning,tRNA,translation},
month = {jun},
number = {1},
pages = {779--812},
pmid = {24499181},
publisher = {Annual Reviews},
title = {{The Scanning Mechanism of Eukaryotic Translation Initiation}},
url = {http://www.annualreviews.org/doi/10.1146/annurev-biochem-060713-035802},
volume = {83},
year = {2014}
