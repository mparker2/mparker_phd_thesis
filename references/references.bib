@article{Nicoludis2012a,
abstract = {The remarkable selectivity of N-Me mesoporphyrin IX (NMM) for G-quadruplexes (GQs) is well-known; however, its ability to stabilize and bind GQs has not been investigated in detail. Through the use of CD, UV-visible spectroscopy and fluorescence resonance energy transfer (FRET) melting assay we have shown that NMM stabilizes human telomeric DNA dAG3(TTAG3)3 (Tel22) and is selective for its parallel conformation to which it binds in 1:1 stoichiometry with a binding const. of ∼1.0 × 105 M-1. NMM does not interact with an antiparallel conformation of Tel22 in sodium buffer and is the second example in the literature, after TOxaPy, of a ligand with an excellent selectivity for a specific GQ structure. NMM's stabilizing ability toward predominantly parallel GQ conformation is universal; it stabilizes a variety of biol. relevant G-rich sequences including telomeres and oncogene promoters. The N-Me group is integral for selectivity and stabilization, as the unmethylated analog, mesoporphyrin IX, does not stabilize GQ DNA in FRET melting assays. Finally, NMM induces the isomerization of Tel22 into a structure with increased parallel component in K+ but not in Na+ buffer. The ability of NMM to cause structural rearrangement and efficient stabilization of Tel22 may bear biol. significance. [on SciFinder(R)]},
author = {Nicoludis, John M. and Barrett, Steven P. and Mergny, Jean Louis and Yatsunyk, Liliya A.},
doi = {10.1093/nar/gks152},
isbn = {0305-1048},
