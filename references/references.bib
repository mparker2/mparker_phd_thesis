publisher = {EMBO Press},
title = {{Exosome cofactor hMTR4 competes with export adaptor ALYREF to ensure balanced nuclear RNA pools for degradation and export}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/28801509 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC5623860 http://emboj.embopress.org/lookup/doi/10.15252/embj.201696139},
