@article{Huppert2005,
abstract = {Guanine-rich DNA sequences of a particular form have the ability to fold into four-stranded structures called G-quadruplexes. In this paper, we present a working rule to predict which primary sequences can form this structure, and describe a search algorithm to identify such sequences in genomic DNA. We count the number of quadruplexes found in the human genome and compare that with the figure predicted by modelling DNA as a Bernoulli stream or as a Markov chain, using windows of various sizes. We demonstrate that the distribution of loop lengths is significantly different from what would be expected in a random case, providing an indication of the number of potentially relevant quadruplex-forming sequences. In particular, we show that there is a significant repression of quadruplexes in the coding strand of exonic regions, which suggests that quadruplex-forming patterns are disfavoured in sequences that will form RNA.},
author = {Huppert, Julian L. and Balasubramanian, Shankar},
doi = {10.1093/nar/gki609},
isbn = {1362-4962 (Electronic)$\backslash$r0305-1048 (Linking)},
issn = {03051048},
journal = {Nucleic Acids Research},
month = {may},
number = {9},
pages = {2908--2916},
pmid = {15914667},
title = {{Prevalence of quadruplexes in the human genome}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/15914667 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC1140081 https://academic.oup.com/nar/article-lookup/doi/10.1093/nar/gki609},
volume = {33},
year = {2005}
}
@article{Tua2000,
abstract = {We present the solution structure of d(CCCTA 2 CCCTA 2 CCCTA 2 CCCT), a fragment of the vertebrate telomere which folds intramolecularly. The four cytidine stretches form an i-motif which includes six intercalated C{\'{A}}C  pairs and terminates with the cytidines at the 5 H extremity of each stretch. Above, the second TA 2 linker loops across one of the narrow grooves, while at the bottom, the {\textregistered}rst and third linkers loop across the wide grooves. At 30 C, the spectra of the {\textregistered}rst and third linkers are quasi-degenerate. Severe broadening at lower temperature indicates that this results from motional averaging between at least two structures of each bottom loop, and makes it impossible to solve the con{\textregistered}guration of the bottom loops directly, in contrast to the rest of the structure. We therefore turned to the modi{\textregistered}ed sequence d(CCCTA 2 5mCCC-TA 2 CCCUA 2 CCCT) in which the two base substitutions (underlined) break the quasi-symmetry between linkers 1 and 3. The three loops follow approximately the hairpin``hairpin``second pattern'' of Hilbers. In the {\textregistered}rst loop, T4 is in the syn orientation, whereas its analog in the third loop, U16, oriented anti, is in a central location, where it interacts with bases of both loops, thus contributing to their tight association. The only motion is a syn/anti ¯ip of A18 in the third loop. Returning to the telomere fragment, we show that each of the bottom loops switches between the structures identi{\textregistered}ed in the {\textregistered}rst and third loops of the modi{\textregistered}ed structure. The motions are concerted, and the resulting con{\textregistered}gurations of the bottom loop cluster present a bulge to either right (T4 syn) or left (T16 syn).},
author = {Tua, Anh and Phan, N and Gue, Maurice and Leroy, Jean-Louis},
