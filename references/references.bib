doi = {10.1038/modpathol.2012.53},
file = {:home/matthew/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/de Wilde et al. - 2012 - Loss of ATRX or DAXX expression and concomitant acquisition of the alternative lengthening of telomeres phenoty.pdf:pdf},
isbn = {0893-3952},
issn = {08933952},
journal = {Modern Pathology},
keywords = {ATRX,DAXX,MEN-1,alternative lengthening of telomeres,multiple endocrine neoplasia-1,pancreatic neuroendocrine tumor},
month = {jul},
number = {7},
pages = {1033--1039},
pmid = {22575867},
publisher = {Nature Publishing Group},
title = {{Loss of ATRX or DAXX expression and concomitant acquisition of the alternative lengthening of telomeres phenotype are late events in a small subset of MEN-1 syndrome pancreatic neuroendocrine tumors}},
