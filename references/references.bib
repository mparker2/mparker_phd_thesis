publisher = {American Chemical Society},
title = {{Optimized End-Stacking Provides Specificity of {\textless}i{\textgreater}N{\textless}/i{\textgreater} -Methyl Mesoporphyrin IX for Human Telomeric G-Quadruplex DNA}},
url = {http://pubs.acs.org/doi/10.1021/ja3088746},
