doi = {10.1021/ja2096255},
file = {:home/matthew/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Mullen, Assmann, Bevilacqua - 2012 - Toward a Digital Gene Response RNA G-Quadruplexes with Fewer Quartets Fold with Higher Cooperativit.pdf:pdf},
