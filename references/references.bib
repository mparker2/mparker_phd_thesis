publisher = {Cold Spring Harbor Laboratory Press},
title = {{Ultrashort and progressive 4sU-tagging reveals key characteristics of RNA processing at nucleotide resolution}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/22539649 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC3460197},
