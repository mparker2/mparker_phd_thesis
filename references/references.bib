journal = {Nature Communications},
keywords = {DNA,Genomics},
month = {dec},
number = {1},
pages = {1796},
pmid = {23653208},
publisher = {Nature Publishing Group},
title = {{G-quadruplex structures are stable and detectable in human genomic DNA}},
url = {http://www.nature.com/articles/ncomms2792},
volume = {4},
year = {2013}
}
